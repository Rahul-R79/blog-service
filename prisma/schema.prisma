generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("SUPABASE_DATABASE_URL")
}

model Post {
  id        String   @id @default(uuid())
  authorId  String   
  title     String
  content   String
  image     String
  category  Int
  readTime Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
